  "devDependencies": {
    "@cosmos-client/core": "^0.45.1",
    "@xchainjs/xchain-client": "^0.11.1",
    "@xchainjs/xchain-crypto": "^0.2.4",
    "@xchainjs/xchain-util": "^0.5.1",
    "haven-core-js": "github:haven-protocol-org/haven-core-js"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@xchainjs/xchain-client": "^0.11.1",
    "@xchainjs/xchain-crypto": "^0.2.4",
    "@xchainjs/xchain-util": "^0.5.1"
  }
